{
  "id": "string",
  "object": "chat.completion",
  "created": integer,                   // timestamp
  "model": "string",
  "choices": [
    {
      "index": integer,
      "message": {
        "role": "assistant",
        "content": "string" | object,       // si JSON mode ou structured output
